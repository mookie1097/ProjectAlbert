/**
* User#Join js file
*/
$("#dialogue").html("<%=escape_javascript(render :partial => 'users/join')%>");
$("#dialogue").css({
    'margin-top': function () { //vertical centering
        return -($(this).height() /2);
    },
    'margin-left': function () { //Horizontal centering
        return -($(this).width() /2);
    }
});

$(".dialogues").show();
$(".dialogues").fadeTo(200, 1.0);

$("#dialogue").css({
    'margin-top': function () { //vertical centering
        return -($(this).height() /2);
    },
    'margin-left': function () { //Horizontal centering
        return -($(this).width() /2);
    }
});